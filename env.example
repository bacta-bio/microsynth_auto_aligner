# Benchling API Configuration Example
# Copy this file to .env and fill in your actual values

# Benchling API Base URL
BENCHLING_BASE_URL=https://api.benchling.com/v2

# Benchling OAuth2 Token URL
BENCHLING_TOKEN_URL=https://api.benchling.com/v2/token

# Benchling OAuth2 Client Credentials
BENCHLING_CLIENT_ID=your_client_id_here
BENCHLING_CLIENT_SECRET=your_client_secret_here

# Benchling UI Domain and Tenant (for constructing sequence URLs)
# Domain: e.g., bacta.benchling.com (without https://)
# Tenant: e.g., bacta (workspace/tenant name in URL path)
BENCHLING_DOMAIN=bacta.benchling.com
BENCHLING_TENANT=bacta

# Request Configuration
REQUEST_TIMEOUT=30
MAX_RETRIES=3
